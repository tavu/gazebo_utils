<?xml version="1.0"?>
<!--
    - Base      : kobuki
    - Stacks    : hexagons
    - 3d Sensor : kinect
-->    
<robot name="turtlebot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find gazebo_utils)/urdf/turtlebot/turtlebot_common_library.urdf.xacro" />
  <xacro:include filename="$(find gazebo_utils)/urdf/kobuki/kobuki.urdf.xacro" />
  <xacro:include filename="$(find gazebo_utils)/urdf/hexagons.urdf.xacro"/>
  <xacro:include filename="$(find gazebo_utils)/urdf/asus_xtion_pro.urdf.xacro"/>  
  
  <kobuki/>
  <stack_hexagons                 parent="base_link"/>
  <sensor_asus_xtion_pro          parent="base_link"/>
</robot>
